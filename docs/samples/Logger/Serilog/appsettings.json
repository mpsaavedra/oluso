{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Logger": {
    "Serilog": {
      "Console": {
        "Enabled": true,
        "MinimumLevel": "Information"
      },
      "File": {
        "Enabled": true,
        "MinimumLevel": "Information",
        "FilePath": "TestLogs\\.log"
      }
    }
  }
}
