{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HealthCheck": {
    "Enabled": true,
    "MemoryHealth": {
      "Enabled": true,
      "Threshold": 1073741824,
      "MemoryHealthCheckName": "memory",
      "MemoryHealthCheckFullName": "hosted_service_startup"
    }
  }
}